<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to your <%= title %></p>
    <% console.log(messages)%>
    <ul>
      <% messages.forEach(function(message){ %>
      <li><%= message.text%></li>
      <li><%= message.user %></li>
      <li><%=message.added%></li>
      <% }) %>
    </ul>
  </body>
</html>
